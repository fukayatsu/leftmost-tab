{
  "name": "Leftmost Tab",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["tabs"],
  "commands": {
    "move-current-tab-to-leftmost": {
      "suggested_key": {
        "default": "Ctrl+Shift+J",
        "mac": "Command+Shift+J"
      },
      "description": "Move current tab to leftmost"
    }
  }
}
